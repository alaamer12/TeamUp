{
  "name": "teamup-server",
  "version": "1.0.1",
  "description": "Backend server for TeamUp application",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "cross-env NODE_ENV=production node src/index.js",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "build": "npm install",
    "lint": "eslint .",
    "test:db": "cross-env NODE_ENV=development node tests/test-update.js",
    "test:api": "cross-env NODE_ENV=development node tests/test-api-update.js",
    "test": "npm run test:db && npm run test:api"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.43.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 